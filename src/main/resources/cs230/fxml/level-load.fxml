<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="backgroundAnchorPane" prefHeight="140.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs230.viewController.LevelLoadController">
   <children>
      <ComboBox fx:id="levelSelector" layoutX="75.0" layoutY="44.0" prefHeight="30.0" prefWidth="200.0" promptText="%levelSelect" style="-fx-background-color: -background-color-dark;" stylesheets="@../css/scoreboard.css" />
      <Text fill="#a49d9d" layoutX="75.0" layoutY="36.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%selectUnlocked" wrappingWidth="220.0" />
      <Button fx:id="load" layoutX="100.0" layoutY="88.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="%load" />
   </children>
   <stylesheets>
      <URL value="@../css/application.css" />
      <URL value="@../css/level-load.css" />
   </stylesheets>
</AnchorPane>
